@if (Gender == GenderType.Male)
{
    <MudIcon Icon="fas fa-male" Size="Size.Small" />
}
else if (Gender == GenderType.Female)
{
    <MudIcon Icon="fas fa-female" Size="Size.Small" />
}
else
{
    <MudIcon Icon="fas fa-question" Size="Size.Small" />
}

@code {
    [Parameter]
    public GenderType Gender { get; set; } = GenderType.Male;
}
